<Window x:Class="QuietTime.Views.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuietTime.Views"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800">
    <StackPanel>
        <Label>hello world</Label>
        <CheckBox IsChecked="{Binding LaunchOnStartup, Mode=OneWayToSource}" Command="{Binding SwitchLaunchOptions}">Open on startup</CheckBox>
        <CheckBox IsChecked="{Binding LaunchMinimized, Mode=OneWayToSource}">Start minimized</CheckBox>
    </StackPanel>
</Window>
