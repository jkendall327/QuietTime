{
  "Settings": {
    "InitialMaxVolume": 50,
    "SerializedDataFilename": "schedules.json",
    "NotificationsEnabled": true,
    "MinimizeOnClose":  true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Error"
    },
    "File": {
      "Path": "app.log",
      "Append": "True",
      "FileSizeLimitBytes": 0, // use to activate rolling file behaviour
      "MaxRollingFiles": 0 // use to specify max number of log files
    }
  }
}